          <gloss versionDate="2007-06-12" xml:lang="fr">parchemin</gloss>
          <gloss versionDate="2007-11-06" xml:lang="it">pergamena</gloss>
          <gloss versionDate="2007-05-04" xml:lang="es">pergamino</gloss>
        </valItem>
        <valItem ident="mixed"/>
      </valList>
