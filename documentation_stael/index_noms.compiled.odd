  </exemplum>
  <exemplum xml:lang="mul">
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <persName>
        <forename>Alfred</forename>
        <nameLink>de</nameLink>
        <surname>Musset</surname>
      </persName>
    </egXML>
  </exemplum>
