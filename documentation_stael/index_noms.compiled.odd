</elementSpec><elementSpec xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="msdescription" xml:id="MSITEM" ident="msItem">
  <gloss versionDate="2007-07-04" xml:lang="en">manuscript item</gloss>
